<div class='container'>
  <div class='header__mobile'>
    <button type='button' class='mobile-button' data-action='open-filter'>
      <span class="material-icons-round icons">menu</span>
      <use href=''></use>
      </svg>
    </button>
    <a href='/' data-action='open-main'>
      <img class='mobile__logo' src='{{this.logo}}' alt='logo' />
    </a>
    <button type='button' class='mobile-button'>
      <span class="material-icons-round icons">search</span>
    </button>
  </div>
  <div class='header__wrapper'>
    <nav class='nav'>
      <a href='/' data-action='open-main'>

        <img class='header__logo logo' src='{{this.logo}}' alt='logo' />
      </a>
      <form class='header__form_desktop'>
        <input class='header__find' data-search='/call/find?search=' type='text' />
        <button class="search__btn" data-search='search'>
          <span class="material-icons icons">search</span>
        </button>
      </form>
    </nav>
    <div class='box'>
      <ul class='list registration log-in' id="register-wraper">
        <li class='item registration__item'>
          <button class='registration__btn-reg' type='button' data-action="open-modal"
            data-value="auth">Регистрация</button>
        </li>
        <li class='item registration__item'>
          <button class='registration__btn-in' type='button' data-action="open-modal" data-value="auth">Войти</button>
        </li>
      </ul>
      <ul class='list cabinet hide' id="cabinet-wraper">
        <li class='item cabinet__item my-cabinet'>
          <a class='cabinet__in' type='button' data-action='open-cabinet'>
            <span class="material-icons-outlined icons">account_circle</span>Мой кабинет</a>
          <div class="modal-cabinet hidden">
            <ul class="menu-caminet">
              <li class="item"><a class="link" href="http://">Мои объявления</a></li>
              <li class="item"><a class="link" href="http://">Избранные</a></li>
            </ul>
          </div>
        </li>
        <li class='item cabinet__item'>
          <button class='cabinet__out' type='button' data-action="log-out" id="logOut">
            <span class="material-icons-round icons">logout</span>
          </button>
        </li>
      </ul>

      <button class='header__btn' type='button' data-action="open-modal" data-value="createEditProduct">Создать
        объявление</button>

    </div>
  </div>
  <div class='filter'>
    <ul class='list filter__list_desktop'>
      {{#each this.data}}
      <li class='item filter__item_desktop'>
        <a href='/call/specific/{{this}}' class='filter__btn'>{{__ this}}</a>
      </li>
      {{/each}}
    </ul>
    <form class='header__form_tablet'>
      <input class='header__find' data-search='/call/find?search=' type='text' />
      <button type='submit' data-search='search' class='search__btn'>
        <span class="material-icons icons">search</span>
      </button>
    </form>
    <button class='filter__btn_open' type='button' data-action='open-filter'>
      <span class="material-icons icons">tune</span>
      Фильтры
    </button>
    <button class='reset-btn' type='reset' data-action='close-filter'>
      <span class="material-icons icons">replay</span>
      Очистить
    </button>
  </div>
</div>
<div class='tablet-menu hidden'>
  <div class='container'>
    <div class='filter_tablet'>
      <ul class='list filter__list_tablet'>
        {{#each this.data}}
        <li class='item filter__item_tablet'>
          <a href='/call/specific/{{this}}' class='filter__btn'>{{__ this}}</a>
        </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
<!-- MOBILE-MENU -->
<div class='mobile-container'>
  <div class='mobile-menu hidden'>
    <button class='btn-close' type='button'>
      <svg class='ikon-close' width='24px' height='24px'>
        <use href=''></use>
      </svg>
    </button>
    <ul class='list log-in mobile-menu__registration'>
      <li class='item registration__item'>
        <button class='registration__btn-reg' type='button'>Регистрация</button>
      </li>
      <li class='item registration__item'>
        <button class='registration__btn-in' type='button'>Войти</button>
      </li>
    </ul>
    <ul class='list cabinet mobile-menu__cabinet'>
      <li class='item cabinet__item menu-cabinet'>
        <button class='cabinet__in' type='button' data-action='open-cabinet-mobile'>
          <svg class='ikon-user' width='20px' height='20px'>
            <use href=''></use>
          </svg>Мой кабинет
        </button>
        <div class="modal-cabinet-mobile hidden">
          <ul class="menu-caminet">
            <li class="item"><a class="link" href="http://">Мои объявления</a></li>
            <li class="item"><a class="link" href="http://">Избранные</a></li>
          </ul>
        </div>
      </li>
      <li class='item cabinet__item'>
        <button class='cabinet__out' type='button'>
          <svg class='ikon-out' width='20px' height='20px'>
            <use href=''></use>
          </svg>
        </button>
      </li>
    </ul>
    <div class='mobile__wrapper'>
      <button class='filter__btn_mobile' type='button'>
        <svg width='24px' height='24px'>
          <use href=''></use>
        </svg>
        Фильтры
      </button>
      <button class='reset-btn_mobile' type='reset' data-action='close-filter'>
        <svg width='24px' height='24px'>
          <use href=''></use>
        </svg>
        Очистить
      </button>
    </div>
    <ul class='list filter__list_mobile'>
      {{#each this.data}}
      <li class='item filter__item_mobile'>
        <a href='/call/specific/{{this}}' class='filter__btn'>{{__ this}}</a>
      </li>
      {{/each}}
    </ul>
  </div>
</div>